KARCH=arm
KERNEL_PATH=arch/${KARCH}/boot/zImage
GCC_FLAGS= #"--with-float=soft"
BINUTILS_FLAGS=
QEMU_TEST=$KARCH
run_emulator()
{
  qemu-system-arm -M versatilepb -nographic -hda "$1" \
	-kernel "$2" -append "$3 console=ttyAMA0"
}

