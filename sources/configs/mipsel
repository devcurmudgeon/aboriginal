CONFIG_FUTEX=y
CONFIG_EPOLL=y
CONFIG_SIGNALFD=y
CONFIG_TIMERFD=y
CONFIG_EVENTFD=y
CONFIG_SHMEM=y
CONFIG_VM_EVENT_COUNTERS=y
CONFIG_SLAB=y
CONFIG_PROC_PAGE_MONITOR=y
CONFIG_BLOCK=y
CONFIG_PCI=y
CONFIG_BINFMT_ELF=y
CONFIG_PM=y
CONFIG_NET=y
CONFIG_PACKET=y
